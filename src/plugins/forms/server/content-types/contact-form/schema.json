{
  "kind": "collectionType",
  "collectionName": "contact_forms",
  "info": {
    "singularName": "contact-form",
    "pluralName": "contact-forms",
    "displayName": "Contact Form"
  },
  "options": {
    "draftAndPublish": false,
    "comment": ""
  },
  "pluginOptions": {
    "content-manager": {
      "visible": false
    },
    "content-type-builder": {
      "visible": false
    }
  },
  "attributes": {
    "contact": {
      "type": "string",
      "required": true
    },
    "passengerName": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "biginteger",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
      },
      "contactBy": {
        "type": "enumeration",
        "enum": [
          "Email",
          "Phone Number"
        ],
        "required": true
      },
    "comments": {
      "type": "string"
    }
  }
}