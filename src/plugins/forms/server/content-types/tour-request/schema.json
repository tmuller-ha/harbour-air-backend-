{
  "kind": "collectionType",
  "collectionName": "tour_requests",
  "info": {
    "singularName": "tour-request",
    "pluralName": "tour-requests",
    "displayName": "Tour Requests"
  },
  "options": {
    "draftAndPublish": false,
    "comment": ""
  },
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Email": {
      "type": "email"
    },
    "Telephone": {
      "type": "biginteger"
    },
    "DepartingLocation": {
      "type": "string",
      "required": true
    },
    "TourType": {
      "type": "enumeration",
      "enum": [
        "Normal Tour",
        "Private Tour"
      ],
      "required": true
    },
    "TourName": {
      "type": "string",
      "required": true
    },
    "Passengers": {
      "type": "integer",
      "required": true
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Time": {
      "type": "time",
      "required": true
    }
  }
}