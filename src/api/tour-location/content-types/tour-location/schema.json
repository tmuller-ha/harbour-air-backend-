{
  "kind": "collectionType",
  "collectionName": "tour_locations",
  "info": {
    "singularName": "tour-location",
    "pluralName": "tour-locations",
    "displayName": "Tour Location",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "heroBackgroundImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "components": {
      "type": "dynamiczone",
      "components": [
        "tours.hero-description",
        "tours.tour-cards",
        "tours.featured-tours",
        "main-tours.tour-cards"
      ]
    },
    "tours": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tour-inside-location-detail.tour-inside-location-detail",
      "mappedBy": "tour_location"
    }
  }
}
