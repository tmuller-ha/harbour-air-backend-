{
  "kind": "collectionType",
  "collectionName": "tour_locations",
  "info": {
    "singularName": "tour-location",
    "pluralName": "tour-locations",
    "displayName": "Tour Location",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "heroBackgroundImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },

    "tours": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tours-detail.tours-detail",
      "mappedBy": "tour_location"
    },
    "departure": {
      "type": "string"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "meta": {
      "type": "component",
      "repeatable": false,
      "component": "meta.meta"
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    }
  }
}
