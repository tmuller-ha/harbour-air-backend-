{
  "kind": "collectionType",
  "collectionName": "tour_inside_location_details",
  "info": {
    "singularName": "tour-inside-location-detail",
    "pluralName": "tour-inside-location-details",
    "displayName": "Tour Inside Location Detail"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "backgroundImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "tourFareDetail": {
      "type": "component",
      "repeatable": true,
      "component": "tours.fare-details"
    },
    "departsText": {
      "type": "string"
    },
    "departLocation": {
      "type": "string"
    },
    "overviewTitle": {
      "type": "string"
    },
    "detailsTitle": {
      "type": "string"
    },
    "detailsDescription": {
      "type": "blocks"
    },
    "overviewDescription": {
      "type": "blocks"
    },
    "tour_location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tour-location.tour-location",
      "inversedBy": "tours"
    }
  }
}
