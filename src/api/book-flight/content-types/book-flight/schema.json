{
  "kind": "singleType",
  "collectionName": "book_flights",
  "info": {
    "singularName": "book-flight",
    "pluralName": "book-flights",
    "displayName": "Book Flights",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "content-manager": {
      "visible": false
    },
    "content-type-builder": {
      "visible": false
    }
  },
  "attributes": {
    "pageTitle": {
      "type": "component",
      "repeatable": false,
      "component": "elements.title-with-image"
    },
    "specialOffers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::deal.deal"
    },
    "specialOffersTitle": {
      "type": "string",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "metaData": {
      "type": "component",
      "repeatable": false,
      "component": "meta-data.meta-data"
    },
    "moreDeals": {
      "type": "component",
      "repeatable": false,
      "component": "elements.button"
    },
    "showSpecialOffers": {
      "type": "boolean",
      "default": false
    }
  }
}
