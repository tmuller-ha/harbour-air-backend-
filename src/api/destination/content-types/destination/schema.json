{
  "kind": "collectionType",
  "collectionName": "destinations",
  "info": {
    "singularName": "destination",
    "pluralName": "destinations",
    "displayName": "Destinations",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "content-manager": {
      "visible": false
    },
    "content-type-builder": {
      "visible": false
    }
  },
  "attributes": {
    "location": {
      "type": "string"
    },
    "locationId": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "tag": {
      "type": "string"
    },
    "Category": {
      "type": "string"
    },
    "latitude": {
      "type": "string",
      "required": true
    },
    "longitude": {
      "type": "string",
      "required": true
    },
    "pickup": {
      "type": "string"
    },
    "pickupDefault": {
      "type": "string"
    },
    "dropOff": {
      "type": "string"
    },
    "dropOffDefault": {
      "type": "string"
    },
    "dropOffRequired": {
      "type": "boolean",
      "default": false
    },
    "pickupRequired": {
      "type": "boolean",
      "default": false
    },
    "map": {
      "type": "string"
    },
    "destinationImg": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "dealsDeparture": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::deal.deal",
      "mappedBy": "departureLocation"
    },
    "dealsArrivals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::deal.deal",
      "mappedBy": "arrivalLocation"
    },
    "tourDepartures": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tour-location.tour-location",
      "mappedBy": "departureLocation"
    },
    "toursDetailsDeparture": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tours-detail.tours-detail",
      "mappedBy": "departureLocation"
    },
    "toursDetailsArrivals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tours-detail.tours-detail",
      "mappedBy": "arrivalLocation"
    }
  }
}
