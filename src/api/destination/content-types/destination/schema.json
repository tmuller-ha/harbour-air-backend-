{
  "kind": "collectionType",
  "collectionName": "destinations",
  "info": {
    "singularName": "destination",
    "pluralName": "destinations",
    "displayName": "Destinations",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "location": {
      "type": "string"
    },
    "locationId": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "tag": {
      "type": "string"
    },
    "Category": {
      "type": "string"
    },
    "latitude": {
      "type": "string",
      "required": true,
      "regex": "/^(\\\\+|-)?(?:90(?:\\\\.0+)?|[1-8]?\\\\d(?:\\\\.\\\\d+)?)$/\""
    },
    "longitude": {
      "type": "string",
      "regex": "/^(\\\\+|-)?(?:180(?:\\\\.0+)?|1[0-7]?\\\\d(?:\\\\.\\\\d+)?|0?\\\\d{1,2}(?:\\\\.\\\\d+)?)$/",
      "required": true
    },
    "pickup": {
      "type": "string"
    },
    "pickupDefault": {
      "type": "string"
    },
    "dropOff": {
      "type": "string"
    },
    "dropOffDefault": {
      "type": "string"
    },
    "dropOffRequired": {
      "type": "boolean",
      "default": false
    },
    "pickupRequired": {
      "type": "boolean",
      "default": false
    },
    "routesDestinations": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::route.route",
      "inversedBy": "destinations"
    },
    "routesArrivals": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::route.route",
      "inversedBy": "arrivals"
    },
    "map": {
      "type": "string"
    }
  }
}
