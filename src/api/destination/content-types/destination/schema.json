{
  "kind": "collectionType",
  "collectionName": "destinations",
  "info": {
    "singularName": "destination",
    "pluralName": "destinations",
    "displayName": "Destinations",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "location": {
      "type": "string"
    },
    "locationId": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "tag": {
      "type": "string"
    },
    "Category": {
      "type": "string"
    },
    "latitude": {
      "type": "string",
      "required": true
    },
    "longitude": {
      "type": "string",
      "required": true
    },
    "pickup": {
      "type": "string"
    },
    "pickupDefault": {
      "type": "string"
    },
    "dropOff": {
      "type": "string"
    },
    "dropOffDefault": {
      "type": "string"
    },
    "dropOffRequired": {
      "type": "boolean",
      "default": false
    },
    "pickupRequired": {
      "type": "boolean",
      "default": false
    },
    "map": {
      "type": "string"
    },
    "routesDeparture": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::route.route",
      "mappedBy": "departure"
    },
    "routesArrivals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::route.route",
      "mappedBy": "arrival"
    },
    "destinationImg": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
